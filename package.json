{
  "name": "@fengqiaogang/dblist",
  "version": "1.0.1",
  "type": "commonjs",
  "description": "Operate on a set of regular list data",
  "main": "build/storage.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "vite build",
    "postbuild": "npm run types",
    "test": "vite",
    "postinstall": "npm shrinkwrap",
    "types": "tsc --declaration --noEmit false --target ESNext --emitDeclarationOnly --declarationDir ./types ./src/index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svonme/DBList.git"
  },
  "keywords": [
    "db",
    "list"
  ],
  "author": {
    "name": "svon",
    "email": "svon.me@gmail.com",
    "url": "https://github.com/svonme"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svonme/DBList/issues"
  },
  "homepage": "https://github.com/svonme/DBList#readme",
  "exports": {
    ".": {
      "import": "./build/storage.js",
      "require": "./build/storage.umd.js"
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "files": [
    "build/",
    "types/",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
